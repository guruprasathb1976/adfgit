{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "dfdsdemo"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Azure_File_To_Local_SQL_Instance",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "FileSystemSource",
								"recursive": false
							},
							"sink": {
								"type": "SqlSink",
								"writeBatchSize": 10000
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"Year": "Year",
									"Product line": "Product line",
									"Product type": "Product type",
									"Product": "Product",
									"Order method type": "Order method type",
									"Retailer country": "Retailer country",
									"Revenue": "Revenue",
									"Planned revenue": "Planned revenue",
									"Product cost": "Product cost",
									"Quantity": "Quantity",
									"Unit cost": "Unit cost",
									"Unit price": "Unit price",
									"Gross profit": "Gross profit",
									"Unit sale price": "Unit sale price"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "Source_File",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "LocalInstance_Guru",
								"type": "DatasetReference"
							}
						]
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Source_File')]",
				"[concat(variables('factoryId'), '/datasets/LocalInstance_Guru')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LocalInstance_Guru')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "SqlServer_local_guru",
					"type": "LinkedServiceReference"
				},
				"type": "SqlServerTable",
				"structure": [
					{
						"name": "Year",
						"type": "Int32"
					},
					{
						"name": "Product line",
						"type": "String"
					},
					{
						"name": "Product type",
						"type": "String"
					},
					{
						"name": "Product",
						"type": "String"
					},
					{
						"name": "Order method type",
						"type": "String"
					},
					{
						"name": "Retailer country",
						"type": "String"
					},
					{
						"name": "Revenue",
						"type": "Decimal"
					},
					{
						"name": "Planned revenue",
						"type": "Decimal"
					},
					{
						"name": "Product cost",
						"type": "Decimal"
					},
					{
						"name": "Quantity",
						"type": "Decimal"
					},
					{
						"name": "Unit cost",
						"type": "Decimal"
					},
					{
						"name": "Unit price",
						"type": "Decimal"
					},
					{
						"name": "Gross profit",
						"type": "Decimal"
					},
					{
						"name": "Unit sale price",
						"type": "Decimal"
					}
				],
				"typeProperties": {
					"tableName": "[concat('[', 'dbo].[AdfDemoTable]')]"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/Source_File')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "FileServer1",
					"type": "LinkedServiceReference"
				},
				"type": "FileShare",
				"structure": [
					{
						"name": "Year",
						"type": "String"
					},
					{
						"name": "Product line",
						"type": "String"
					},
					{
						"name": "Product type",
						"type": "String"
					},
					{
						"name": "Product",
						"type": "String"
					},
					{
						"name": "Order method type",
						"type": "String"
					},
					{
						"name": "Retailer country",
						"type": "String"
					},
					{
						"name": "Revenue",
						"type": "String"
					},
					{
						"name": "Planned revenue",
						"type": "String"
					},
					{
						"name": "Product cost",
						"type": "String"
					},
					{
						"name": "Quantity",
						"type": "String"
					},
					{
						"name": "Unit cost",
						"type": "String"
					},
					{
						"name": "Unit price",
						"type": "String"
					},
					{
						"name": "Gross profit",
						"type": "String"
					},
					{
						"name": "Unit sale price",
						"type": "String"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": true
					},
					"fileName": "WA_Retail-SalesMarketing_-ProfitCost.csv",
					"folderPath": ""
				}
			}
		}
	]
}